      <Button fx:id="start" layoutX="156.0" layoutY="173.0" mnemonicParsing="false" onAction="#start" prefHeight="57.0" prefWidth="191.0" text="Start Game">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="gl" layoutX="93.0" layoutY="371.0" mnemonicParsing="false" onAction="#GLeader" prefHeight="57.0" prefWidth="319.0" text="Global Leaderboard">
