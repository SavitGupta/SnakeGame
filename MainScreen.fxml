      <Label fx:id="username" alignment="CENTER" layoutX="17.0" layoutY="83.0" prefHeight="21.0" prefWidth="466.0" textAlignment="CENTER" textFill="#c4cafc">
         <font>
            <Font name="AR JULIAN" size="36.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
