      <Label alignment="CENTER" layoutX="17.0" layoutY="42.0" prefHeight="21.0" prefWidth="466.0" text="Hi!" textAlignment="CENTER" textFill="#ffffff">
         <font>
            <Font name="AR JULIAN" size="36.0" />
